{
  "name": "AtCoderEnv",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "customizations": {
    "vscode": {
      "settings": {
        "terminal.integrated.defaultProfile.linux": "bash",
        "editor.tabSize": 4
      },
      "extensions": [
        "ms-python.python",
        "ms-python.vscode-pylance",
        "rust-lang.rust-analyzer"
      ]
    }
  },
  "postCreateCommand": "python3.13 --version && pypy3 --version && rustc --version"
}
